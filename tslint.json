{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-sonarts"],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single"],
    "max-line-length": [true, 120],
    "no-console": [false, "log", "error"],
